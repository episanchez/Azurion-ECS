{
	"tags": {
		"allowUnknownTags" : true
	},
	"opts": {
		"destination" : "./doc"
		},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles" : true
		},
		"applicationName": "Yeoku",
		"disqus": "",
		"googleAnalytics": "",
